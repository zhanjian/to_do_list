- if notice.present?
  div.alert.alert-sucess.alert-dismissable
    button(type="button" class="close" data-dismiss="alert" aria-hidden="true")
      ' &times
    = notice
- if alert.present?
  div.alert.alert-dismissable.alert-danger
    button(type="button" class="close" data-dismiss="alert" aria-hidden="true")
      ' &times
    = alert
